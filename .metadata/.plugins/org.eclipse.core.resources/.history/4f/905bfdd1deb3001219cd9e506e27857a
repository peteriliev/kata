package com.piliev;

import com.google.inject.Guice;
import com.google.inject.Injector;

public class Tester {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		final Injector jin = Guice.createInjector(new  Module());
		MyService srv = jin.getInstance(MyServiceImpl.class);
		
		srv.m1();
		
	}
}
